<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Sdkmanager_java"
        parent="Theme.MaterialComponents.DayNight.DarkActionBar">

        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/green_35</item>
        <item name="colorPrimaryVariant">@color/green_35_10</item>
        <item name="colorOnPrimary">@color/white_100</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/yellow_75</item>
        <item name="colorSecondaryVariant">@color/yellow_75</item>
        <item name="colorOnSecondary">@color/black_20</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>