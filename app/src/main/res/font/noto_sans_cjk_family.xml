<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:targetApi="o">

    <!--  thin  -->
    <font
        android:font="@font/noto_sans_cjk_kr_thin"
        android:fontStyle="normal"
        android:fontWeight="100"

        app:font="@font/noto_sans_cjk_kr_thin"
        app:fontStyle="normal"
        app:fontWeight="100" />

    <!--  extra light  -->
    <font
        android:font="@font/noto_sans_cjk_kr_light"
        android:fontStyle="normal"
        android:fontWeight="200"

        app:font="@font/noto_sans_cjk_kr_light"
        app:fontStyle="normal"
        app:fontWeight="200" />

    <!--  light  -->
    <font
        android:font="@font/noto_sans_cjk_kr_demi_light"
        android:fontStyle="normal"
        android:fontWeight="300"

        app:font="@font/noto_sans_cjk_kr_demi_light"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <!--  regular  -->
    <font
        android:font="@font/noto_sans_cjk_kr_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/noto_sans_cjk_kr_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!--  medium  -->
    <font
        android:font="@font/noto_sans_cjk_kr_medium"
        android:fontStyle="normal"
        android:fontWeight="500"

        app:font="@font/noto_sans_cjk_kr_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--  bold  -->
    <font
        android:font="@font/noto_sans_cjk_kr_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/noto_sans_cjk_kr_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!--  bold  -->
    <font
        android:font="@font/noto_sans_cjk_kr_black"
        android:fontStyle="normal"
        android:fontWeight="900"

        app:font="@font/noto_sans_cjk_kr_black"
        app:fontStyle="normal"
        app:fontWeight="900" />

</font-family>